<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>facility.servelets.admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addfacilityservelet</servlet-name>
        <servlet-class>facility.servelets.addfacilityservelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>facility.servelets.delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addF</servlet-name>
        <servlet-class>facility.servelets.addF</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updatefacilitiesSevelet</servlet-name>
        <servlet-class>facility.servelets.updatefacilitiesSevelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addfacilityservelet</servlet-name>
        <url-pattern>/addfacilityservelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addF</servlet-name>
        <url-pattern>/addF</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updatefacilitiesSevelet</servlet-name>
        <url-pattern>/updatefacilitiesSevelet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
